<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/post_match"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PostMatch">

    <EditText
        android:id="@+id/edit_Name"
        android:layout_width="336dp"
        android:layout_height="95dp"
        android:autofillHints="Type your name"
        android:ems="10"
        android:inputType="text"
        android:text="Name"
        tools:ignore="HardcodedText,LabelFor,MissingConstraints,VisualLintBounds"
        tools:layout_editor_absoluteX="335dp"
        tools:layout_editor_absoluteY="129dp" />

    <Button
        android:id="@+id/but_SubmitPage1"
        android:layout_width="238dp"
        android:layout_height="97dp"
        android:text="Submit"
        android:textSize="25sp"
        tools:layout_editor_absoluteX="477dp"
        tools:layout_editor_absoluteY="431dp"
        tools:ignore="HardcodedText,MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>