<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/settings"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".Settings">

    <TextView
        android:id="@+id/text_Competition"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Competition:"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="19dp"
        tools:ignore="HardcodedText,MissingConstraints" />

    <EditText
        android:id="@+id/edit_CompetitionId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:autofillHints="Enter Competition Id here"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="233dp"
        tools:layout_editor_absoluteY="15dp"
        tools:ignore="LabelFor,MissingConstraints" />

    <TextView
        android:id="@+id/text_CompetitionName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="574dp"
        tools:layout_editor_absoluteY="18dp"
        tools:ignore="HardcodedText,MissingConstraints" />

    <TextView
        android:id="@+id/text_Device"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Device:"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="19dp"
        tools:ignore="HardcodedText,MissingConstraints" />

    <EditText
        android:id="@+id/edit_DeviceId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:autofillHints="Enter Device Id here"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="233dp"
        tools:layout_editor_absoluteY="15dp"
        tools:ignore="LabelFor,MissingConstraints" />

    <TextView
        android:id="@+id/text_DeviceName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        tools:layout_editor_absoluteX="574dp"
        tools:layout_editor_absoluteY="18dp"
        tools:ignore="HardcodedText,MissingConstraints" />

    <Button
        android:id="@+id/but_Cancel"
        android:layout_width="189dp"
        android:layout_height="62dp"
        android:text="Cancel"
        tools:ignore="hardCodedText, missingConstraints"
        tools:layout_editor_absoluteX="755dp"
        tools:layout_editor_absoluteY="522dp" />

    <Button
        android:id="@+id/but_Save"
        android:layout_width="111dp"
        android:layout_height="55dp"
        android:text="Save"
        tools:ignore="hardCodedText, missingConstraints"
        tools:layout_editor_absoluteX="619dp"
        tools:layout_editor_absoluteY="521dp" />

</androidx.constraintlayout.widget.ConstraintLayout>