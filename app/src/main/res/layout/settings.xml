<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/settings"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".Settings">

    <TextView
        android:id="@+id/text_Competition"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Competition:"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="19dp"
        tools:ignore="hardCodedText, missingConstraints,VisualLintBounds,VisualLintOverlap" />

    <EditText
        android:id="@+id/edit_CompetitionId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:autofillHints="Enter Competition Id here"
        android:ems="10"
        android:inputType="number"
        tools:ignore="LabelFor,MissingConstraints,DuplicateClickableBoundsCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck,VisualLintOverlap,VisualLintBounds"
        tools:layout_editor_absoluteX="233dp"
        tools:layout_editor_absoluteY="15dp" />

    <TextView
        android:id="@+id/text_CompetitionName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CompName"
        tools:ignore="hardCodedText, missingConstraints,VisualLintBounds"
        tools:layout_editor_absoluteX="504dp"
        tools:layout_editor_absoluteY="38dp" />

    <TextView
        android:id="@+id/text_Device"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Device:"
        tools:ignore="hardCodedText, missingConstraints,VisualLintBounds"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="151dp" />

    <EditText
        android:id="@+id/edit_DeviceId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:autofillHints="Enter Device Id here"
        android:ems="10"
        android:inputType="number"
        tools:ignore="LabelFor,MissingConstraints,SpeakableTextPresentCheck,TouchTargetSizeCheck,VisualLintBounds"
        tools:layout_editor_absoluteX="233dp"
        tools:layout_editor_absoluteY="125dp" />

    <TextView
        android:id="@+id/text_DeviceName"
        android:layout_width="190dp"
        android:layout_height="0dp"
        android:text="DeviceName"
        tools:ignore="hardCodedText, missingConstraints,VisualLintBounds"
        tools:layout_editor_absoluteX="504dp"
        tools:layout_editor_absoluteY="139dp" />

    <Button
        android:id="@+id/but_Cancel"
        android:layout_width="189dp"
        android:layout_height="62dp"
        android:text="Cancel"
        tools:ignore="hardCodedText, missingConstraints,VisualLintBounds"
        tools:layout_editor_absoluteX="436dp"
        tools:layout_editor_absoluteY="245dp" />

    <Button
        android:id="@+id/but_Save"
        android:layout_width="111dp"
        android:layout_height="55dp"
        android:text="Save"
        tools:ignore="hardCodedText, missingConstraints,VisualLintBounds"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="252dp" />

</androidx.constraintlayout.widget.ConstraintLayout>